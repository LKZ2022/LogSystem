# LoggingLibraryConfig.cmake.in - CMAKE 配置文件，用于查找 LoggingLibrary 包

# cmake/LoggingLibraryConfig.cmake.in


@PACKAGE_INIT@

set_and_check(LOGGING_INCLUDE_DIR "@PACKAGE_INCLUDE_INSTALL_DIR@")
set_and_check(LOGGING_LIBRARY_DIR "@PACKAGE_LIB_INSTALL_DIR@")

include("${CMAKE_CURRENT_LIST_DIR}/LoggingLibraryTargets.cmake")

# 提供查找包的结果变量
set(LOGGING_FOUND TRUE)
set(LOGGING_INCLUDE_DIRS ${LOGGING_INCLUDE_DIR})
set(LOGGING_LIBRARIES LoggingLibrary)

# 检查静态库标志
if(LOGGING_STATIC_DEFINED)
    set(LOGGING_STATIC TRUE)
else()
    set(LOGGING_STATIC FALSE)
endif()